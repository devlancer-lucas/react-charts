    "d3-array": "^3.0.1",
    "d3-scale": "^4.0.0",
    "d3-shape": "^3.0.1",
    "d3-time": "^3.0.0",
    "jotai": "^1.1.2",
    "ts-toolbelt": "^9.6.0"
  },
