{
  "name": "react-charts",
  "author": "Tanner Linsley",
  "description": "Charts for React",
  "repository": "tannerlinsley/react-charts",
  "module": "dist/react-charts-1.esm.js",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build-d3": "rollup -c d3/rollup.config.js",
    "start": "yarn build-d3 && tsdx watch --transpileOnly",
    "build": "yarn build-d3 && tsdx build --transpileOnly",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build-d3 && tsdx build --transpileOnly",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "next",
        "prerelease": true
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/react-charts-1.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-charts-1.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/d3-shape": "^2.1.0",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@reach/observe-rect": "^1.2.0",
    "@react-spring/web": "^9.2.3",
    "@types/d3-array": "^3.0.1",
    "@types/d3-delaunay": "^6.0.0",
    "@types/d3-scale": "^3.3.0",
    "@types/raf": "^3.4.0",
    "@visx/axis": "^1.14.0",
    "@visx/grid": "^1.14.0",
    "@visx/shape": "^1.14.0",
    "@visx/text": "^1.10.0",
    "@visx/voronoi": "^1.7.0",
    "d3-array": "^3.0.1",
    "d3-delaunay": "^6.0.2",
    "d3-scale": "^4.0.0",
    "d3-shape": "^2.1.0",
    "d3-time": "^2.1.1",
    "jotai": "^1.0.0",
    "performance-now": "^2.1.0",
    "raf": "^3.4.1",
    "react-popper": "^2.2.5",
    "ts-toolbelt": "^9.6.0"
  }
}
